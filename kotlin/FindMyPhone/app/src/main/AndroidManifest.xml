<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vikeshpatil.findmyphone">
<uses-permission android:name="android.permission.READ_CONTACTS"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/dmap"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dmap"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Trackers" android:label="Contact"></activity>
        <activity android:name=".Login" android:label="Login"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>